{
  "name": "base-angular-universal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rm -rf",
    "clean:all": "yarn run clean:vendors && yarn run clean:dist",
    "clean:vendors": "yarn run clean node_modules",
    "clean:dist": "yarn run clean dist",
    "clean:install": "yarn run clean:all && yarn",
    "start": "yarn run start:dev",
    "start:base": "yarn run update:serve && ng serve",
    "start:dev": "yarn run start:base --configuration development",
    "start:prod": "yarn run start:base --configuration production",
    "start:ssr": "ng run base-angular-universal:serve-ssr --configuration development",
    "build": "yarn run build:dev",
    "build:base": "ng build --extra-webpack-config webpack.partial.js",
    "build:dev": "yarn run build:base --configuration development",
    "build:dev:ssr": "yarn run build:dev && ng run base-angular-universal:server",
    "build:prod": "yarn run build:base --configuration production",
    "build:prod:ssr": "yarn run build:prod && ng run base-angular-universal:server",
    "serve:ssr": "node dist/server/main.js",
    "script:app": "node scripts/app.js --file src/app.json",
    "script:pages": "node scripts/pages.js",
    "update:serve": "yarn run script:app --type serve",
    "update:patch": "yarn run script:app --type patch",
    "update:minor": "yarn run script:app --type minor",
    "update:major": "yarn run script:app --type major",
    "bundle:analyzer": "webpack-bundle-analyzer dist/stats-es2015.json",
    "prerender": "ng run base-angular-universal:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/platform-server": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@exalif/ngx-breadcrumbs": "^9.2.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@hackages/ngxerrors": "^8.0.0",
    "@nguniversal/express-engine": "^12.1.0",
    "angular2-ladda": "^3.2.0",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.1.0",
    "crypto-es": "^1.2.7",
    "js-cookie": "^3.0.0",
    "ladda": "^2.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "ngx-bootstrap": "^7.1.0",
    "ngx-build-plus": "^12.2.0",
    "ngx-moment": "^5.0.0",
    "ngx-pipes": "^2.7.5",
    "ngx-toastr": "^14.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.3",
    "@angular/cli": "~12.2.3",
    "@angular/compiler-cli": "~12.2.0",
    "@nguniversal/builders": "^12.1.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "commander": "^8.1.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ncp": "^2.0.0",
    "node-run-cmd": "^1.0.1",
    "typescript": "~4.3.5"
  }
}
